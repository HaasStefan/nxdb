{
  "name": "@nx-db/parser",
  "version": "0.1.3",
  "description": "NxQL parser for NxDB",
  "keywords": [
    "nxdb",
    "parser",
    "nx",
    "nxql",
    "database"
  ],
  "author": "Stefan Haas <stefan.haas.privat@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HaasStefan/nxdb.git",
    "directory": "packages/parser"
  },
  "bugs": {
    "url": "https://github.com/HaasStefan/nxdb/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/HaasStefan/nxdb#readme",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "targets": {
      "peggy": {
        "executor": "nx:run-commands",
        "cache": false,
        "options": {
          "command": "peggy --format es --output src/lib/parser/parser.js src/lib/parser/parser.pegjs",
          "cwd": "packages/parser"
        }
      }
    }
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "@nx-db/db": "^0.1.3",
    "chalk": "^5.4.1",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "peggy": "^5.0.4"
  }
}
