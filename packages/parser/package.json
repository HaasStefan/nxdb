{
  "name": "@nxdb/parser",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "targets": {
      "peggy": {
        "executor": "nx:run-commands",
        "options": {
          "command": "peggy --format es --output src/lib/parser/parser.js src/lib/parser/parser.pegjs",
          "cwd": "packages/parser"
        },
        "inputs": [
          "^packages/parser/src/parser/parser.pegjs"
        ],
        "outputs": [
          "{projectRoot}/src/parser/parser.js"
        ]
      }
    }
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "peggy": "catalog:"
  }
}
